Nbcol <- ncol(data)
n <- 1 #nombre de lignes dans le data.frame final
for(c in 4:Nbcol)
{
for(l in 1:Nbligne)
{
if(data[l,c]!=0)
{
date[n] <- names(data.hellfest)[c]
scene[n] <- data[l,c]
groupe[n] <- data.hellfest[l,1]
n <- n+1
}
}
}
R <- data.frame(date,scene,groupe)
return(R)
}
